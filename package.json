{
  "name": "squadcalc",
  "version": "29.0.1",
  "description": "A Complete Mortar Calculator for Squad",
  "author": "Maxime 'sharkman' Boussard",
  "license": "MIT",
  "keywords": [
    "squad",
    "squadcalc",
    "mortar",
    "calculator"
  ],
  "repository": {
    "type": "git",
    "url": "git:/github.com/sh4rkman/SquadCalc"
  },
  "homepage": "https://squadcalc.app/",
  "bugs": {
    "url": "https://github.com/sh4rkman/SquadCalc/issues"
  },
  "private": false,
  "jshintConfig": {
    "esversion": 7
  },
  "scripts": {
    "start": "webpack serve --open --config ./config/webpack.common.js",
    "build": "npm run bundle && node -e \"require('fs').cpSync('./public/', './dist/', {recursive: true});\"",
    "bundle": "webpack --config ./config/webpack.common.js",
    "lint": "stylelint -c ./config/.stylelintrc.json ./src/css/*.scss && htmlhint --nocolor ./src/ && html-validator --quiet --continue ./src/ && jshint ./src/ && eslint -c ./config/.eslintrc.json ./src/",
    "mapinfo": "node src/js/tests/mapdata.js info",
    "weaponInfo": "node src/js/tests/calcMortarTech.js info"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.1",
    "@types/node": "^20.8.10",
    "animate.css": "^4.1.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.34.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.3",
    "htmlhint": "^1.1.4",
    "i18next": "^23.11.5",
    "i18next-http-backend": "^2.5.2",
    "jquery": "^3.7.1",
    "jshint": "^2.13.6",
    "leader-line-new": "^1.1.9",
    "leaflet": "^1.9.4",
    "leaflet-edgebuffer": "^1.0.6",
    "leaflet.gridlayer.fadeout": "^0.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": ">=8.4.31",
    "purecss": "^3.0.0",
    "robotstxt-webpack-plugin": "^8.0.0",
    "sass": "^1.69.4",
    "sass-loader": "^12.6.0",
    "select2": "^4.1.0-rc.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "tippy.js": "^6.3.7",
    "w3c-html-validator": "^1.3.2",
    "webpack": "^5.80.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^7.0.0"
  }
}
